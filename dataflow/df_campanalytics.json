{
	"name": "df_campanalytics",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "campaignanalyticscsv",
					"description": "This source files contains the data in CSV format"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sqltable_campaignanalytics",
						"type": "DatasetReference"
					},
					"name": "targetsql"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Region as string,",
				"          Country as string,",
				"          Product_Category as string,",
				"          Campaign_Name as string,",
				"          Revenue as string,",
				"          Revenue_Target as string,",
				"          City as string,",
				"          State as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> campaignanalyticscsv",
				"campaignanalyticscsv sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Region as string,",
				"          Country as string,",
				"          ProductCategory as string,",
				"          CampaignName as string,",
				"          Revenue as decimal(10,2),",
				"          RevenueTarget as decimal(10,2),",
				"          City as string,",
				"          State as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> targetsql"
			]
		}
	}
}